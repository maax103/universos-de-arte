{
  "name": "vite-react-ts-minimal-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
